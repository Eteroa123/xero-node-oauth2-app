<!doctype html>
<html>
  <%- include('shared/head') -%>

  <body>
    <%- include('shared/nav') -%>
    <div class='core-content'>
      <%- include('shared/header') -%>
      <div class='body-wrapper'>
        <div class='body-content'>
          <p><strong>Folders:</strong>
          <ul>
            <% for(var i=0; i < folders.length; i++) { %>
              <li><%- JSON.stringify(folders[i], null, 2).replace(/[{}]/g, '') %></li>
              <br>
            <% } %>
          </ul>
          <p>Created Folder: <%= createFolder %></p>
        </div>
      </div>
    </div>
  </body>
</html>
